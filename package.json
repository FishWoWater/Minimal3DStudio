{
  "name": "minimal_3d_studio",
  "author": "FishWoWater", 
  "description": "A minimal 3D studio for 3D generative models.",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": ".",
  "dependencies": {
    "@react-three/drei": "^10.4.2",
    "@react-three/fiber": "^9.2.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/three": "^0.178.0",
    "axios": "^1.10.0",
    "electron-is-dev": "^3.0.1",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scripts": "5.0.1",
    "three": "^0.178.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "cross-env NODE_ENV=production electron .",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development ELECTRON_IS_DEV=true electron .\"",
    "electron-pack": "npm run build && cross-env NODE_ENV=production electron-builder",
    "electron-pack-dev": "npm run build && cross-env CSC_IDENTITY_AUTO_DISCOVERY=false NODE_ENV=production electron-builder",
    "preelectron-pack": "npm run build",
    "setup-icons": "./setup-icons.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "electron": "^37.2.0",
    "electron-builder": "^26.0.12",
    "electron-reload": "^1.5.0",
    "typescript": "^4.9.5",
    "wait-on": "^8.0.3"
  },
  "build": {
    "appId": "com.minimal3dstudio.app",
    "productName": "Minimal 3D Studio",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "!node_modules/**/test/**/*",
      "!node_modules/**/tests/**/*",
      "!node_modules/**/*.test.*",
      "!node_modules/**/*.spec.*",
      "!node_modules/**/.*",
      "!node_modules/**/README*",
      "!node_modules/**/CHANGELOG*",
      "!node_modules/**/LICENSE*",
      "!node_modules/**/docs/**/*",
      "!node_modules/**/examples/**/*"
    ],
    "mac": {
      "icon": "build/icon.icns",
      "category": "public.app-category.graphics-design",
      "identity": null
    },
    "win": {
      "icon": "build/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "requestedExecutionLevel": "asInvoker"
    },
    "linux": {
      "icon": "build/icon.png"
    },
    "compression": "maximum",
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Minimal 3D Studio",
      "runAfterFinish": true,
      "installerIcon": "build/icon.ico",
      "uninstallerIcon": "build/icon.ico",
      "installerHeaderIcon": "build/icon.ico",
      "deleteAppDataOnUninstall": false,
      "allowElevation": true,
      "perMachine": false
    }
  }
}
